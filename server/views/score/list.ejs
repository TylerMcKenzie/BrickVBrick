<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/page/head') %>
  </head>
  <body>
    <%- include('../partials/page/nav') %>

    <div class="scores-container">
      <div class="title">
        <h4>HighScores</h4>
      </div>

      <div class="scores">
        <ul>
          <% for(var i=0; i<scores.length; i++) { %>
            <li>
              <div class="user">
                <%= scores[i].User.username %>
              <div>

              <div class="score">
                <%= scores[i].score %>
              </div>

              <div class="date">
                <%= scores[i].date.toLocaleString() %>
              </div>
            </li>
          <% }; %>
        </ul>
      </div>
    </div>

    <%- include('../partials/page/scripts') %>
  </body>
</html>
